/**app.wxss**/
.container {
  height: 100%;
  padding: 0rpx;
  margin: 0rpx;
  
} 
page{
  background: #F5F5F5
}

.hoverClass{
  background: rgba(245,245,245, .6) !important;
}

/* mask */
.mask{
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: 2;
  top: 0;
  bottom: 0;
  background: rgba(7,59,86,.8);
}


/* 列表 */
.listItem{
  width: 49%;
  margin: 1rpx .5%;
  display: flex;
  flex-direction: column;
  overflow: hidden;
  height: 500rpx;
  align-items: center ;
  background: white;
}
.listItem image{
  width: 90%;
  height: 320rpx;
  margin: 20rpx 5%;
}
.listItemText{
  padding: 10rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 24rpx;
  font-weight: 600;
  text-align: center;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  
}
.listItemTexts{
  font-size: 23rpx;
  font-weight: 100 !important;
}











